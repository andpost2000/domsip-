head(itemscope itemtype="http://schema.org/WebSite")
	meta(charset="utf-8")
	meta(http-equiv="x-ua-compatible", content="ie=edge")
	meta(name="viewport", content="width=device-width, initial-scale=1.0")
	meta(content="telephone=no", name="format-detection")

	meta(name='yandex-verification', content='99afe47646df7e30')

	meta(property='og:locale', content='ru_RU')
	meta(property='og:type', content='website')
	meta(property='og:title', content='Стильный дом из СИП панелей')
	meta(property='og:description', content='Строительство домов из СИП панелей. Канадская технология, качественные материалы и лучшие специалисты, гарантия успешного проекта.')
	meta(property='og:url', content='https://domsip.by')
	meta(property='og:image', content='https://domsip.by/static/img/general/meta.jpg')
	meta(property='og:image:type', content='image/jpg')
	meta(property='og:image:width', content='300')
	meta(property='og:image:height', content='200')

	meta(name="HandheldFriendly", content="true")
	link(href="%=static=%css/main%=hash=%%=min=%.css" rel="stylesheet" type="text/css")
	link(rel="icon" type="image/x-icon" href="favicon.ico")

	block head
	title= pageTitle || ''
	meta(name='description' content=pageDescription || '')
	meta(name='keywords' content=pageKeywords || '')
	script(src='%=static=%js/separate-js/jquery.min.js')
	script(src='%=static=%js/separate-js/flipclock.min.js')
